{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/api/health",
      "dest": "index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/test-db",
      "dest": "index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/api/cron/reset",
      "dest": "index.js"
    },
    {
      "src": "/api/cron/announce",
      "dest": "index.js"
    },
    {
      "src": "/admin",
      "dest": "index.js"
    },
    {
      "src": "/image",
      "dest": "index.js"
    },
    {
      "src": "/action",
      "dest": "index.js"
    },
    {
      "src": "/login",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
} 