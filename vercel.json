{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/test-db",
      "dest": "index.js"
    },
    {
      "src": "/admin",
      "dest": "index.js"
    },
    {
      "src": "/api/cron/reset",
      "dest": "index.js"
    },
    {
      "src": "/api/cron/announce",
      "dest": "index.js"
    },
    {
      "src": "/api/health",
      "dest": "index.js"
    },
    {
      "src": "/image",
      "dest": "index.js"
    },
    {
      "src": "/action",
      "dest": "index.js"
    },
    {
      "src": "/login",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    }
  ],
  "cron": {
    "reset": {
      "path": "/api/cron/reset",
      "schedule": "0 0 * * *"
    },
    "announce": {
      "path": "/api/cron/announce",
      "schedule": "0 20 * * *"
    }
  }
} 